{
  "name": "iview-project",
  "version": "2.0.0",
  "description": "A base project with Vue.js2、Vue-Router、webpack2 and with iView2.",
  "main": "index.js",
  "scripts": {
    "init": "webpack --progress --config webpack.dev.config.js",
    "dev": "webpack-dev-server --content-base ./ --open --inline --hot --compress --history-api-fallback --config webpack.dev.config.js",
    "build": "webpack --progress --hide-modules --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iview/iview-project.git"
  },
  "author": "Aresn",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.2.6",
    "vue-router": "^2.2.1",
    "iview": "^2.0.0-rc.18"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "axios": "^0.16.2",
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "vanilla-tilt": "^1.4.0",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^13.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "bugs": {
    "url": "https://github.com/iview/iview-project/issues"
  },
  "homepage": "https://www.iviewui.com"
